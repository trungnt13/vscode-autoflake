{
  "name": "autoflake",
  "displayName": "autoflake",
  "description": "Remove unused imports in Python",
  "publisher": "TrungNgo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autoflake.removeUnused"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoflake.removeUnused",
        "title": "Autoflake: Remove Unused Imports",
        "when": "editorTextFocus && !editorReadonly && editorLangId == 'python'"
      }
    ],
    "configuration": [
      {
        "title": "Autoflake",
        "properties": {
          "autoflake.removeAllUnusedImports": {
            "type": "boolean",
            "default": true,
            "scope": "application",
            "description": "Remove all unused imports"
          },
          "autoflake.removeAllUnusedVariables": {
            "type": "boolean",
            "default": false,
            "scope": "application",
            "description": "Remove all unused variables"
          },
          "autoflake.removeDuplicateKeys": {
            "type": "boolean",
            "default": false,
            "scope": "application",
            "description": "Remove duplicated keys in objects"
          },
          "autoflake.path": {
            "type": "string",
            "default": "autoflake",
            "scope": "application",
            "description": "Path to autoflake binary"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
